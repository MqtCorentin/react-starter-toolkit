{
  "apiUrl": "https://react-starter-toolkit-api.netlify.app/api/",
  "baseUrl": "",
  "fetchConfig": {
    "crossDomain": true,
    "withCredentials": true,
    "method": "GET",
    "Access-Control-Allow-Origin": "*",
    "headers": {
      "accept": "application/json"
    }
  },
  "oidc": {
    "isEnabled": true,
    "client_id": "interactive.public",
    "redirect_uri": "http://toolkitreact.com:8080/authentication/callback",
    "response_type": "code",
    "post_logout_redirect_uri": "http://toolkitreact.com:8080/",
    "scope": "openid profile email api offline_access",
    "authority": "https://demo.identityserver.io",
    "silent_redirect_uri": "http://toolkitreact.com:8080/authentication/silent_callback",
    "automaticSilentRenew": true,
    "loadUserInfo": true
  }
}
